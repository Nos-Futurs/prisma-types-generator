{
  "name": "@nos-futurs/prisma-types-generator",
  "version": "1.3.4",
  "description": "Generates DTO and Entity classes from Prisma Schema for NestJS",
  "license": "Apache-2.0",
  "author": "Yan Marinich & Nos Futurs",
  "bin": {
    "prisma-types-generator": "dist/cli.js"
  },
  "bugs": {
    "url": "https://github.com/Nos-Futurs/prisma-types-generator/issues"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/Nos-Futurs/prisma-types-generator",
  "keywords": [
    "prisma",
    "generator",
    "nestjs",
    "dto",
    "typescript",
    "swagger",
    "openapi",
    "entity"
  ],
  "main": "dist/index.js",
  "packageManager": "^npm@7.7.6",
  "prepublish": "dts build --target node --format cjs --rollupTypes",
  "publishConfig": {
    "access": "private"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nos-Futurs/prisma-types-generator.git"
  },
  "scripts": {
    "build": "npm run clean && tsc --project tsconfig.build.json",
    "clean": "rimraf src/@generated",
    "format": "prettier --write \"{src,test}/**/*.{js,ts}\"",
    "generate": "npm run clean && npx prisma generate && npm run format -- --loglevel error",
    "lint": "npm run format -- --loglevel error && eslint \"{src,test}/**/*.{js,ts}\" --fix",
    "patch": "standard-version --release-as patch",
    "push": "git push --follow-tags origin",
    "release": "standard-version && npm run push",
    "test": "npm run lint && jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@prisma/generator-helper": "^3.15.2",
    "@prisma/internals": "^4.7.1",
    "case": "^1.6.3",
    "make-dir": "^3.1.0",
    "prettier": "^2.6.2",
    "slash": "^3.0.0",
    "tar": "^6.1.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.1",
    "@commitlint/config-conventional": "^18.4.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.1",
    "@types/prettier": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^27.2.4",
    "lint-staged": "^15.1.0",
    "prisma": "^3.15.2",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^4.4.3"
  }
}